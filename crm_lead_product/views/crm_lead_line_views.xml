<?xml version="1.0" ?>
<odoo>
    <record id="view_tree_lead_line" model="ir.ui.view">
        <field name="name">crm.lead.form.lead</field>
        <field name="model">crm.lead.line</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="product_id" string="Produto"/>
                <field name="category_id" string="Categoria" />
                <field name="product_tmpl_id" string="Descricao" />
                <field name="name" widget="section_and_note_text" string="Descricao" />
                <field name="product_qty" string="Quantidade" />
                <field name="price_unit" string="Preco Unitario" />
                <field name="expected_revenue" string="Receita Esperada" />
                <field name="prorated_revenue" string="Receita Rateada" />
            </tree>
        </field>
    </record>
</odoo>
