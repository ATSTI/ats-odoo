<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_repair_date_order_tree_view" model="ir.ui.view">
            <field name="name">repair.date.order.tree</field>
            <field name="model">repair.order</field>
            <field name="inherit_id" ref="repair.view_repair_order_tree" />
            <field name="arch" type="xml">
                <field name="name" position="after">
                    <field name="date_repair"/>
                    <field name="data_validacao" />
                </field>
            </field>
        </record>
        <record id="view_repair_date_order_form" model="ir.ui.view">
            <field name="name">repair.order.form</field>
            <field name="model">repair.order</field>
            <field name="inherit_id" ref="repair.view_repair_order_form" />
            <field name="arch" type="xml">
                <field name="guarantee_limit" position="before"> 
                    <field name="date_repair" />
                    <field name="data_validacao" />
                    <field name="date_repair_closed" />
                </field>
            </field>
        </record>
    </data>
</odoo>